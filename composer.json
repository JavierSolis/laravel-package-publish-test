{
    "name": "javierlibs/laravel-publish-test-package",
    "description": "Package Laravel to test publication with none function",
    "keywords": [
        "javierlibs",
        "laravel-publish-test-package"
    ],
    "homepage": "https://github.com/javierlibs/laravel-publish-test-package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Javier Solis",
            "email": "none@domain.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "JavierLibs\\LaravelPublishTestPackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JavierLibs\\LaravelPublishTestPackage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JavierLibs\\LaravelPublishTestPackage\\LaravelPublishTestPackageServiceProvider"
            ],
            "aliases": {
                "LaravelPublishTestPackage": "JavierLibs\\LaravelPublishTestPackage\\LaravelPublishTestPackageFacade"
            }
        }
    }
}
